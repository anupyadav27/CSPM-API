{
	"printWidth": 100,
	"tabWidth": 4,
	"useTabs": true,
	"singleQuote": false,
	"semi": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"arrowParens": "always",
	"overrides": [
		{
			"files": ["*.js", "*.ts", "*.tsx", "*.jsx"],
			"options": {
				"useTabs": false,
				"tabWidth": 4
			}
		}
	]
}
